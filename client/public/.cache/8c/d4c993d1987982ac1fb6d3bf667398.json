l":{"line":103,"column":0},"generated":{"line":111,"column":19}},{"source":"../src/components/TaskList.vue","name":null,"original":{"line":103,"column":0},"generated":{"line":111,"column":22}},{"source":"../src/components/TaskList.vue","name":null,"original":{"line":103,"column":0},"generated":{"line":111,"column":23}},{"source":"../src/components/TaskList.vue","name":null,"original":{"line":103,"column":0},"generated":{"line":111,"column":28}},{"source":"../src/components/TaskList.vue","name":null,"original":{"line":103,"column":0},"generated":{"line":111,"column":36}},{"source":"../src/components/TaskList.vue","name":null,"original":{"line":104,"column":0},"generated":{"line":112,"column":0}},{"source":"../src/components/TaskList.vue","name":null,"original":{"line":62,"column":0},"generated":{"line":113,"column":0}},{"source":"../src/components/TaskList.vue","name":null,"original":{"line":39,"column":0},"generated":{"line":114,"column":0}}],"sources":{"../src/components/TaskList.vue":"<template>\n    <div class=\"column\">\n        <div class=\"title\">\n            <h5>{{ category }}</h5>\n        </div>\n        \n        <TaskItem v-for=\"task in getTasksByCategory\" :key=\"task.id\" \n        :task=\"task\"\n        :UserId=\"UserId\"\n        @itemRemove=\"removeItem\"></TaskItem>\n        \n        <div class=\"newForm\">\n            <div class=\"btnFormTask\"  v-on:click=\"showForm\">\n                <i class=\"material-icons small\">add</i>Add new task\n            </div>\n            <div v-if=\"seenForm\">\n                <form v-on:submit.prevent=\"createTask\">\n                    <div class=\"row\">\n                        <div class=\"input-field\">\n                            <input placeholder=\"Task\" type=\"text\" v-model=\"newTask\">\n                            <label >Task</label>\n                        </div>\n                        <div class=\"input-field col s6\">\n                            <button type=\"submit\" class=\"btn\">Create</button>\n                        </div>\n                        <div class=\"input-field col s6\">\n                            <button class=\"btn\" type=\"reset\">Clear</button>\n                        </div>\n                    </div>\n                </form>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport TaskItem from './TaskItem';\nimport { url } from '../utils.js';\nexport default {\n    name: 'TaskList',\n    data(){\n        return {\n            newTask: '',\n            seenForm: false\n        }\n    },\n    props: ['category', 'tasks', 'UserId'],\n    components: {\n        TaskItem\n    },\n    computed: {\n        getTasksByCategory: function(){\n            let temp = [];\n            this.tasks.forEach(i => {\n                i.category === this.category ? temp.push(i) : '';\n            })\n            return temp;\n        }\n    },\n    created() {\n    },\n    methods: {\n        createTask: function() {\n            axios({\n                url: url + `/tasks`,\n                method: 'POST',\n                data: {\n                    title: this.newTask,\n                    category: this.category\n                },\n                headers: {\n                    access_token: localStorage.getItem('access_token')\n                }\n            })\n            .then((response) => {\n                this.newTask = '';\n                this.seenForm = !this.seenForm\n                Swal.fire(\n                'Success!',\n                'You have create new task!',\n                'success'\n                );\n                this.$emit('itemFromNewTask', response.data);\n            })\n            .catch((err) => {\n                Swal.fire({\n                    icon: 'error',\n                    title: 'Oops...',\n                    text: err.response.data.message,\n                    showClass: {\n                        popup: 'animated fadeInDown faster'\n                    },\n                    hideClass: {\n                        popup: 'animated fadeOutUp faster'\n                    }\n                });\n            });\n        },\n        removeItem: function(payload){\n            this.$emit('itemRemove', payload);\n        },\n        showForm: function(){\n            this.seenForm = !this.seenForm\n        }\n    },\n};\n</script>"},"lineCount":null}},"error":null,"hash":"d016829695f207ee0cbe0738477a4d4a","cacheData":{"env":{}}}